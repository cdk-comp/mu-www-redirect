{
  "name": "cdk-comp/mu-www-redirect",
  "type": "wordpress-muplugin",
  "license": "MIT",
  "description": "Redirect non-www to www on production environment",
  "homepage": "https://github.com/cdk-comp/mu-www-redirect/",
  "keywords": ["wordpress"],
  "support": {
    "issues": "https://github.com/cdk-comp/mu-www-redirect/issues/",
    "forum": "https://github.com/cdk-comp/mu-www-redirect/"
  },
  "autoload": {
    "psr-4": {
      "Roots\\Bedrock\\": "src/"
    }
  },
  "require": {
    "php": ">=7.0.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.3.2"
  },
  "scripts": {
    "test": ["phpcs --ignore=vendor --extensions=php --standard=PSR2 ."]
  }
}
